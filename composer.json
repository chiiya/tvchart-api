{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "ext-zlib": "*",
        "chiiya/filament-access-control": "^2.4",
        "chiiya/laravel-tmdb": "^1.3",
        "chiiya/laravel-utilities": "^5.1",
        "filament/filament": "^3.2",
        "guzzlehttp/guzzle": "^7.8",
        "laravel/framework": "^11.6",
        "laravel/horizon": "^5.24",
        "nwidart/laravel-modules": "^11.0",
        "sentry/sentry-laravel": "^4.5",
        "sokil/php-isocodes": "^4.2",
        "sokil/php-isocodes-db-only": "^4.0",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-query-builder": "^5.8",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "chiiya/laravel-code-style": "^2.3",
        "fakerphp/faker": "^1.23",
        "laravel/telescope": "^5.0",
        "laravel/tinker": "^2.9",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.1",
        "spatie/laravel-db-snapshots": "^2.6",
        "spatie/laravel-ignition": "^2.6",
        "timacdonald/log-fake": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope",
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpro/grumphp": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
